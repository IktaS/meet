{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~icons/*": ["node_modules/.vite/deps/~icons__*"],
      "@/*": ["src/*"]
    },
    "types": ["src/types"],
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "src/types", "astro.config.mjs"],
  "exclude": ["node_modules", "dist", ".astro"]
} 